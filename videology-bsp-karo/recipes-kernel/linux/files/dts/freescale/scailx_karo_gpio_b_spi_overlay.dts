// SPI1 overlay to use SPI bus, which is on GPIO-B interface

/dts-v1/;
/plugin/;

&{/chosen} {
	overlays {
		gpio_b_spi = __TIMESTAMP__;
	};
};

&{/} {
    aliases {
		gpio-b-spi = &spidev_b;
	};
};

&gpio_b_spi {
	status = "okay";
	spidev_b: spi@0 {
		reg = <0>;
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <30000000>;
	};
};
